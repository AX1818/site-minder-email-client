{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/prefer-stateless-function": 0,
        "react/jsx-boolean-value": [1, "always"],
        "no-use-before-define": 0,
        "semi": "warn",
        "quotes": ["warn", "single"],
        "no-undef": "off",
        "no-console": "off",
        "no-alert": "off"
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]

}